---
# tasks file for building_server
- name: Ensure maven-package is present
  apt:
    name: maven
    state: present
- name: Ensure maven service is started
  shell: "mvn clean package"
   
   
- name: Synchronization using rsync protocol 
  synchronize:
    src: "{{ source_folder }}"
    dest: rsync://root@62.84.17.211/{{ dest_folder }}
  delegate_to: 62.84.117.211
